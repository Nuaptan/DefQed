Cell[TextData[{
 "Significantly, ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[ScriptCapitalC]", "t"], TraditionalForm]],ExpressionUUID->
  "af3f04e0-804b-4720-862f-0694ce2d5eb6"],
