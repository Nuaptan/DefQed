 RowBox[{
  RowBox[{"\[ForAll]", "X"}], ",", 
  RowBox[{"Y", " ", "\[Element]", 
   RowBox[{"Ob", 
    RowBox[{"(", "\[ScriptCapitalC]", ")"}]}]}], ",", 
  RowBox[{"\[ForAll]", 
   RowBox[{"h", "\[Element]", 
    RowBox[{
     SubscriptBox["Hom", "\[ScriptCapitalC]"], 
